                    </id>
                    <font>
                        <Font name="Arial Narrow" size="18.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleButtonsMenuPrincipalAction" prefHeight="25.0" prefWidth="461.0" text="%RulesGame">
